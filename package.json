{
  "name": "help-promise",
  "version": "1.0.0",
  "description": "",
  "main": "help-promise.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "rollup-bundle": "rollup -c",
    "babel-rollup": "babel --out-dir ./ --source-maps --presets latest,import-export --plugins add-module-exports,transform-es2015-modules-umd dist/help-promise.js",
    "uglify": "uglifyjs --compress cascade,join_vars,if_return,unused,loops,booleans,evaluate,comparisons,conditionals,drop_debugger,sequences,properties,dead_code --mangle -o dist/help-promise.min.js dist/help-promise.js",
    "build": "npm run rollup-bundle && npm run babel-rollup && npm run uglify"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-import-export": "^1.0.2",
    "babel-preset-latest": "^6.24.1",
    "chai": "^3.5.0",
    "mocha": "^3.2.0"
  }
}
